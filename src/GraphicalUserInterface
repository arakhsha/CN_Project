from tkinter import *
from src.UserInterface import UserInterface


class GraphicsUserInterFace(UserInterface):

    def __init__(self):
        UserInterface.__init__(self)
        if type == '1':
            self.is_root = True
        else:
            self.is_root = False




if __name__ == "__main__":
    gui = GraphicsUserInterFace()
    gui.run()